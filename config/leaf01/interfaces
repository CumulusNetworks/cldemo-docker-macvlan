# /etc/network/interfaces template for switches (leaf01)
# Created by Ansible

auto lo
iface lo inet loopback
  address 10.0.0.11/32

auto eth0
iface eth0 inet dhcp

auto swp2
iface swp2

auto swp1
iface swp1
  address 172.16.1.1/24
  post-up ip link set $IFACE promisc on

auto swp51
iface swp51

auto swp52
iface swp52

